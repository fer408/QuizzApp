<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.quizzapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="Conspiracy Theory Quizz App"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScreenActivities.ScreenOneActivity" />
        <activity android:name=".ScreenActivities.ScreenTwoActivity" />
        <activity android:name=".ScreenActivities.ScreenThreeActivity" />
        <activity android:name=".ScreenActivities.ScreenFourActivity" />
        <activity android:name=".ScreenActivities.ScreenFiveActivity"></activity>
    </application>

</manifest>